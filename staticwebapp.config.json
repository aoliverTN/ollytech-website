{
  "routes": [
    {
      "route": "/privacy",
      "redirect": "/privacy-policy.html"
    },
    {
      "route": "/about-us",
      "redirect": "/about.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "Referrer-Policy": "strict-origin-when-cross-origin"
  }
}
